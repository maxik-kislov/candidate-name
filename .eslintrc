{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "^16.12.0"
    }
  },
  "rules": {
    "no-undef": "off",
    "no-unused-vars": "off"
  }
}
